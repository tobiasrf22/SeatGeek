  padding: 10px;
  float: right;
  color:white;
  border-radius: 10px;
  margin-top: -19px;
  margin-right: 10px;
  font-weight:bold;
  font-size: 15px;
  cursor:pointer;


}
.tickets button:hover{
  background-color:#1762bf;
}

.tickets:hover{

  background-color: #EEF5FF;
  border-top-left-radius: 10px;
  border-bottom-left-radius: 10px;
}

.stadium{
  margin-right: -70px;
  margin-top: -75px;
  float: right;
  transform: scale(0.7);
}
.ticketss{
  transition: 1s;
}
.header{
  width: all;
  height: 60px;
}
.header button{
